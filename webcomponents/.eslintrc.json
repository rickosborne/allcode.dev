{
	"extends": [
		"plugin:@typescript-eslint/recommended",
		"plugin:json/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"plugins": [
		"sort-keys-fix",
		"json"
	],
	"rules": {
		"array-bracket-newline": [
			"error",
			"consistent"
		],
		"json/json": "error",
		"sort-keys-fix/sort-keys-fix": "error"
	}
}
